<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>List Page</h1>
    <a href="/insert">INSERT DATA</a>
    <hr>
    <table width="100%" border="1">
        <tr>
            <th>DELETE</th>
            <th>EDIT</th>
            <th>ID</th>
            <th>NAME</th>
            <th>MODEL NUMBER</th>
            <th>SERIES</th>
        </tr>
        <% data.forEach(function(item, index) { %>
        <tr>
            <th><a href="/delete/<%= item.id %>">DELETE</a></th>
            <th><a href="/edit/<%= item.id %>">EDIT</a></th>
            <th><%= item.id %></th>
            <th><%= item.name %></th>
            <th><%= item.modelnumber %></th>
            <th><%= item.series %></th>
        </tr>   
        <% }); %>
    </table>
</body>
</html>